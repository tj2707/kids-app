{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch API",
      "program": "${workspaceRoot}/api/node_modules/.bin/serverless",
      "args": [
        "offline",
        "start",
        "--port",
        "3001",
        "--skipCacheInvalidation"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "outFiles": [
        "${workspaceRoot}/api/dist/*.js"
    ]
    }
 ]
}
